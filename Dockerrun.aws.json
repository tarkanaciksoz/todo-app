{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "frontend",
            "ContainerName": "todo-app-frontend",
            "image": "tarkanaciksoz/todo-app-frontend:latest",
            "essential": true,
            "environment": [
                {
                    "name": "ENV",
                    "value": "prod"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "memory": 500
        }
    ]
}